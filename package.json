{
  "name": "animenode-js-front",
  "version": "1.0.0",
  "description": "Anime Render server written in Node.JS using server side rendering of React components.",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/nathancyam/anime-app-frontend-react"
  },
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha -R dot --compilers js:babel-register --require babel-polyfill --require tests/bootstrap.js tests/**/*Test.js",
    "test.ci": "NODE_ENV=test mocha -R mocha-junit-reporter --compilers js:babel-register --require babel-polyfill --require tests/bootstrap.js tests/**/*Test.js",
    "build": "npm run build.server && npm run build.client",
    "build.prod": "NODE_ENV=production npm run build",
    "build.server": "NODE_ENV=production webpack --config webpack.server.config.js",
    "build.client": "TARGET=build webpack -p --config webpack.production.js",
    "serve": "TARGET=dev nodemon build/server.bundle.js",
    "start": "if-env NODE_ENV=production && npm run start.prod || npm run start.dev",
    "start.dev": "concurrently --kill-others \"npm run webpack\" \"npm run watch.server\" \"npm run serve\"",
    "start.prod": "npm run build && node server.bundle.js",
    "webpack": "TARGET=dev webpack-dev-server",
    "watch.server": "TARGET=dev webpack --watch --config webpack.server.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.6.4",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "bootswatch": "^3.3.6",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.1.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.2",
    "http-proxy": "^1.11.2",
    "istanbul": "^0.4.2",
    "jsdom": "^9.4.1",
    "mocha": "^3.1.2",
    "mocha-junit-reporter": "^1.13.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.9.1",
    "postcss-loader": "^1.1.0",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^4.0.0",
    "serviceworker-webpack-plugin": "^0.2.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^0.15.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.5",
    "font-awesome": "^4.3.0",
    "immutable": "^3.7.4",
    "isomorphic-fetch": "^2.1.1",
    "lodash": "^4.6.1",
    "morgan": "^1.8.1",
    "npm": "^3.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.0.0",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.0",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "socket.io-client": "^1.4.5",
    "webpack": "^3.0.0"
  }
}
